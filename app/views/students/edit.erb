<div class="container">
  <%= render partial: '/partial/errors', layout: false, locals: {errors: @errors}%>
  <h5>Update password</h5>
  <%= form_for  @student do |f| %>

    <%= hidden_field_tag :email, @student.email,  class: 'form-control'%>

    <%= f.label :password%>
    <%= f.password_field :password, class: 'form-control'%><br>

    <%= f.submit "Update", class: "btn btn-primary" %>
  <% end %>
</br><a href="/home" class="btn btn-primary">Cancel</a>


</div>
