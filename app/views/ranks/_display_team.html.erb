<div class="container">
    <% Cohort.last.setting.number_in_second_round.times do |idx| %>
        <div class="col-md-6 team" >
            <div class="">
              <span><%= teams[idx][:pitch]%></span>
            </div>
          <div id="team<%=idx+1%>" class='col-md-6'>
            <% teams[idx][:student].each do |student|%>
              <span class='student'><%= student.first_name %></span>
            <% end %>
          </div>
        </div>
    <% end %>
</div
